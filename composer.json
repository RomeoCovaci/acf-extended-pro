{
    "name": "kubeecms/acf-extended-pro",
    "type": "wordpress-plugin",
    "license": "GNU",
    "description": "All-in-one enhancement suite that improves Custom Fields",
    "homepage": "https://github.com/KubeeCMS/acf-extended-pro/",
    "keywords": ["kubeecms", "kubee", "kcms-drive"],
    "support": {
        "issues": "https://github.com/KubeeCMS/acf-extended-pro/issues/",
        "forum": "https://discourse.kubee.com/"
    },
    "autoload": {
        "psr-4": {
            "Roots\\Bedrock\\": "/"
        }
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.1.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
    }
}